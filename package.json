{
	"name": "lostnfound",
	"version": "1.0.0",
	"description": "Lost and Found application for Codeshastra",
	"main": "server.js",
	"type": "module",
	"scripts": {
		"start": "node backend/server",
		"server": "nodemon backend/server",
		"client": "npm start --prefix frontend",
		"data:import": "node backend/seeder",
		"data:destroy": "node backend/seeder -d",
		"dev": "concurrently \"npm run server\" \"npm run client\""
	},
	"author": "Nitish",
	"license": "MIT",
	"dependencies": {
		"bcryptjs": "^2.4.3",
		"colors": "^1.4.0",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.17.1",
		"express-async-handler": "^1.1.4",
		"express-graphql": "^0.12.0",
		"graphql": "^15.4.0",
		"ioredis": "^4.20.0",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.10.6",
		"mongoose-fuzzy-searching": "^2.0.2",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"razorpay": "^2.0.6"
	},
	"devDependencies": {
		"chai": "^4.2.0",
		"chai-http": "^4.3.0",
		"concurrently": "^5.3.0",
		"eslint": "^6.8.0",
		"eslint-config-airbnb": "^18.2.1",
		"eslint-config-node": "^4.1.0",
		"eslint-config-prettier": "^7.2.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.3.1",
		"mocha": "^8.2.1",
		"nodemon": "^2.0.4",
		"prettier": "^2.2.1"
	}
}
